<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/salatronowa"
    tools:context=".afterMenu">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="34dp">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/salatronowa">


            <Button
                android:id="@+id/asasynButton"
                android:layout_width="129dp"
                android:layout_height="44dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="moveToKiller"
                android:text="ZABOJCA"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.944"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.845" />

            <pl.droidsonroids.gif.GifImageView
                android:id="@+id/gifImageView"
                android:layout_width="274dp"
                android:layout_height="345dp"

                android:layout_marginEnd="212dp"
                android:src="@drawable/warrior"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.655" />


            <pl.droidsonroids.gif.GifImageView
                android:id="@+id/gifImageView2"
                android:layout_width="257dp"
                android:layout_height="329dp"

                android:src="@drawable/king"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.475">

            </pl.droidsonroids.gif.GifImageView>

            <Button
                android:id="@+id/HpButton"
                android:layout_width="195dp"
                android:layout_height="57dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="NextQuest"
                android:text="Nastepne zadanie"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.759" />

            <TextView
                android:id="@+id/textViewGold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2000000"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.109"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.007" />

            <Button
                android:id="@+id/Button"
                android:layout_width="214dp"
                android:layout_height="50dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="WerbunekClick"
                android:text="REKRUTUJ ZBROJNYCH"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.512"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.185" />

            <TextView
                android:id="@+id/textViewGoldWatch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/magic"
                android:text="Zloto"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.321"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.008" />

            <TextView
                android:id="@+id/textViewArmyWatch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/magic"
                android:text="Wojsko"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.714"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textViewArmy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="5000"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.862"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/button"
                android:layout_width="130dp"
                android:layout_height="75dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="BossQuest"
                android:text="Walka ze smokiem"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.941"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.975" />

            <Button
                android:id="@+id/buttonCityUP"
                android:layout_width="148dp"
                android:layout_height="85dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="moveToCityUpgrade"
                android:text="Rozbudowa miasta"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.148"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.952" />

            <Button
                android:id="@+id/ButtonWork"
                android:layout_width="214dp"
                android:layout_height="50dp"
                android:background="@drawable/pattern1"
                android:fontFamily="@font/magic"
                android:onClick="onClickWork"
                android:text="PRACUJ"
                android:textColor="#EAE6E6"
                android:textColorHighlight="#FFFFFF"
                android:textColorHint="#FF0000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.512"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.089" />

        </android.support.constraint.ConstraintLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>